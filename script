dirs=`find -name "UNIQUE" -type d`
#dirs=`find -name "SAVED" -type d`
dirs=`find -name "*" -type d -maxdepth 1`
cwd="`pwd`"

for d in $dirs
do
cd $d
pwd 
pwd >>$cwd/str-counts
echo ALL/*.res |wc -w >>$cwd/str-counts
#mv *.res ../
#cryan-analyse.py -cl 4:800  -t c -nopl -pres 1e1 -save -np 8 &>script.out &
echo UNIQUE/*.res |wc -w >>$cwd/str-counts
#mkdir -p ALL; mv *.res C.* ALL

cd $cwd
done
